<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bryce.mymusicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sixties"
            android:label="Sixties Albums" />
        <activity
            android:name=".seventies"
            android:label="Seventies Albums"/>
        <activity android:name=".eighties"
            android:label="Eighties Albums"/>
        <activity
            android:name=".nineties"
            android:label="Nineties Albums"/>
    </application>

</manifest>